<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Maven Repository</os:ShortName>
<os:Description>Searches the central maven repository containing Java libraries released via the Maven 2 build system.</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/mvn_repository.xml</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/mvn_repository.png</IconUpdateUrl>
<SearchForm>http://mvnrepository.com</SearchForm>
<os:Url type="text/html" method="GET" template="http://mvnrepository.com/search.html?query={searchTerms}">
</os:Url>
</SearchPlugin>