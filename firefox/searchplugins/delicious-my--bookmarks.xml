<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/" xmlns:os="http://a9.com/-/spec/opensearch/1.1/">
<os:ShortName>Delicious (My  bookmarks)</os:ShortName>
<os:Description>Searches in your del.icio.us bookmarks (if you are logged in)</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<UpdateInterval>7</UpdateInterval>
<UpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/your_del.icio.us.xml</UpdateUrl>
<IconUpdateUrl>http://mycroft.mozdev.org/updateos.php/id0/your_del.icio.us.ico</IconUpdateUrl>
<SearchForm>http://del.icio.us/search/</SearchForm>
<os:Url type="text/html" method="GET" template="http://del.icio.us/search/?p={searchTerms}&amp;type=user">
</os:Url>
</SearchPlugin>