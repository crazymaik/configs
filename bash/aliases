#!/bin/bash

alias oo="ls"
alias ll="ls -lh"
alias aa="ls -lh"
alias mk="mkdir -p"
alias u="j"
alias c="cd"

alias a="arun"

# git
alias ga="git add"
alias gl='git pull'
alias gp='git push'
alias gd='git diff'
alias gc='git commit'
alias gca='git commit -a'
alias gcm='git commit -m'
alias gcam='git commit -a -m'
alias gco='git checkout'
alias gb='git branch'
alias gs='git status'
alias grm='git rm'
alias gdel="git status | grep 'deleted:' | awk '{print \$3}' | xargs git rm"
alias qg='qgit --all'
